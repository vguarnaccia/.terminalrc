alias gs='git status'
alias ga='git add'
alias gd='git diff'
alias gdc='git diff --cached'
function gc(){
    git commit -m "$*"
}
alias gl="git log --graph --pretty=format:'%C(yellow)%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'" # pretty log
alias gpl='git pull'
alias gpu='git push'
alias gbr='git branch -vv'
alias gst='git stash'
